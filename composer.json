{
    "name": "armezit/omnipay-jibit",
    "type": "library",
    "description": "Jibit driver for the Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "jibit"
    ],
    "homepage": "https://github.com/armezit/omnipay-jibit",
    "license": "MIT",
    "authors": [
        {
            "name": "Armin Rezayati",
            "email": "armin.rezayati@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\Jibit\\" : "src/" }
    },
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "omnipay/common": "^3"
    },
    "require-dev": {
        "omnipay/tests": "^4",
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "^3",
        "phpstan/phpstan": "^1.8"
    },
    "prefer-stable": true,
    "minimum-stability": "stable",
    "scripts": {
        "lint": "phpcs",
        "test": "phpunit"
    }
}
